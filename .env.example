# LifeEase Environment Configuration Template
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control

# ===========================================
# Firebase Configuration (from google-services.json / GoogleService-Info.plist)
# ===========================================
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_APP_ID=your_firebase_app_id_here
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_PROJECT_ID=your_firebase_project_id_here
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket_here

# iOS-specific Firebase settings (optional, used for iOS builds)
FIREBASE_IOS_API_KEY=your_ios_api_key_here
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_CLIENT_ID=your_ios_client_id_here
FIREBASE_IOS_BUNDLE_ID=your_ios_bundle_id_here

# ===========================================
# Agora Configuration (for video calls)
# ===========================================
AGORA_APP_ID=your_agora_app_id_here

# ===========================================
# API Keys (for future integrations)
# ===========================================
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# ===========================================
# Environment Settings
# ===========================================
# Options: development, staging, production
APP_ENV=development

# ===========================================
# App Settings
# ===========================================
APP_NAME=LifeEase
API_BASE_URL=https://your-backend-url.com
ENABLE_NOTIFICATIONS=true
