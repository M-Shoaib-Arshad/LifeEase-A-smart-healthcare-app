# LifeEase Environment Configuration
# Copy this file to .env and fill in your actual values

# =====================================================
# Agora Configuration (Required for Video Calls)
# =====================================================
# Get your App ID from: https://console.agora.io/
AGORA_APP_ID=your_agora_app_id_here

# Optional: Token server URL for production (leave empty for development)
AGORA_TOKEN_SERVER_URL=

# =====================================================
# Firebase Configuration
# =====================================================
# Note: Firebase config is already in firebase_options.dart
# These are for reference only

# =====================================================
# Backend API (Optional)
# =====================================================
# Your custom backend API URL (if you have one)
API_BASE_URL=https://your-backend-api.com

# =====================================================
# AI/ML Services (Optional)
# =====================================================
# OpenAI API Key for AI health recommendations
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Google Gemini API Key (alternative to OpenAI)
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=

# =====================================================
# Environment Settings
# =====================================================
# Options: development, staging, production
ENVIRONMENT=development

# Enable debug logging (true/false)
DEBUG_MODE=true

# =====================================================
# Feature Flags (Optional)
# =====================================================
# Enable/disable specific features
ENABLE_TELEMEDICINE=true
ENABLE_AI_RECOMMENDATIONS=false
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_ANALYTICS=true

# =====================================================
# Third-Party Services (Optional)
# =====================================================
# Stripe for payments (if implementing payment features)
STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=

# SendGrid for email notifications (if using custom email service)
SENDGRID_API_KEY=

# Twilio for SMS (if implementing SMS notifications)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=
